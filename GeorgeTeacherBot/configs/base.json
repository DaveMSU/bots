{
    "agent": {
        "name": "ProbabilisticQLearningWordAgent",
        "params": {
            "core_parameters": {
                "model_type": "RandomForestClassifier",
                "params": {
                    "n_estimators": 100,
                    "criterion": "entropy"
                }
            },
            "knowledge_threshold": 0.41,
            "epsilon": 0.1,
            "max_vector_len": 50,
            "init_qvalue": 0.01,
            "softmax_t": 0.01
        }
    },
    "data_pretrain_path": "GTT.ChatLog",
    "path_to_word_base": "GTT.WordBase"
}
